                    </InvokeMethod.TargetObject>
                    <InArgument x:TypeArguments="sd:DataRow">[row]</InArgument>
                  </InvokeMethod>
                </Sequence>
              </If.Then>
            </If>
          </Sequence>
        </ActivityAction>
      </ui:ForEachRow.Body>
    </ui:ForEachRow>
    <If Condition="[Selected_Items.Rows.Count &gt; 0]" sap2010:WorkflowViewState.IdRef="If_3">
      <If.Then>
